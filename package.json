{
  "name": "skni-kod",
  "description": "Student Research Group of Computer Science - \"Code\".",
  "homepage": "https://kod.prz.edu.pl/",
  "bugs": {
    "url": "https://github.com/mateuszaliyev/skni-kod/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mateusz Aliyev",
    "url": "https://github.com/mateuszaliyev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mateuszaliyev/skni-kod"
  },
  "scripts": {
    "build": "pnpm i18n:build && pnpm prisma:build && pnpm next:build",
    "dev": "concurrently \"npm:*:dev\"",
    "format": "prettier --config ./prettier.config.js --write \"**/*.{css,js,md,mjs,scss,ts,tsx}\"",
    "i18n:build": "typesafe-i18n --no-watch",
    "i18n:dev": "typesafe-i18n",
    "lint": "next lint",
    "next:build": "next build",
    "next:dev": "next dev",
    "prisma": "prisma",
    "prisma:build": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio -b none",
    "start": "next start",
    "typecheck": "tsc",
    "vscode:generate": "pnpm dlx tsx ./scripts/vscode.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.0.3",
    "@prisma/client": "4.6.1",
    "@tanstack/react-query": "4.16.1",
    "@trpc/client": "10.0.0-rc.8",
    "@trpc/next": "10.0.0-rc.8",
    "@trpc/react-query": "10.0.0-rc.8",
    "@trpc/server": "10.0.0-rc.8",
    "@vercel/og": "0.0.20",
    "clsx": "1.2.1",
    "nanoid": "4.0.0",
    "next": "13.0.3",
    "next-auth": "4.16.4",
    "next-themes": "0.2.1",
    "nextjs-cors": "2.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "superjson": "1.11.0",
    "trpc-openapi": "1.0.0-alpha.4",
    "typesafe-i18n": "5.16.3",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.9",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "autoprefixer": "10.4.13",
    "concurrently": "7.5.0",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys": "2.3.5",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "4.6.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "private": true
}
