  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mateuszaliyev/skni-kod"
  },
  "scripts": {
    "build": "pnpm i18n:build && pnpm prisma:build && pnpm next:build",
    "dev": "concurrently \"npm:*:dev\"",
    "format": "prettier --config ./prettier.config.js --write \"**/*.{css,js,md,mjs,scss,ts,tsx}\"",
    "i18n:build": "typesafe-i18n --no-watch",
    "i18n:dev": "typesafe-i18n",
    "lint": "next lint",
    "next:build": "next build",
    "next:dev": "next dev",
    "prisma": "prisma",
    "prisma:build": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio -b none",
    "start": "next start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@next/bundle-analyzer": "12.3.1",
    "@prisma/client": "4.5.0",
    "@tanstack/react-query": "4.13.0",
    "@trpc/client": "10.0.0-proxy-beta.25",
    "@trpc/next": "10.0.0-proxy-beta.25",
    "@trpc/react": "10.0.0-proxy-beta.20",
    "@trpc/server": "10.0.0-proxy-beta.25",
    "@vercel/og": "0.0.20",
    "nanoid": "4.0.0",
    "next": "12.3.1",
    "next-auth": "4.15.0",
    "next-themes": "0.2.1",
    "nextjs-cors": "2.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.11.0",
    "trpc-openapi": "1.0.0-alpha.3",
    "typesafe-i18n": "5.15.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "18.11.3",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "autoprefixer": "10.4.12",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys": "2.3.5",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "4.5.0",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4"
  },
  "private": true
}
